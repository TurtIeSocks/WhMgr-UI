<script type='text/javascript'>
    {{#analytics}}
    window.ga = window.ga || function () {
        (ga.q = ga.q || []).push(arguments);
    }
    ga.l = +new Date();
    ga('create', '{{analytics}}', 'auto');
    ga('send', 'pageview');
    {{/analytics}}
    {{#adsense}}
    (adsbygoogle = window.adsbygoogle || []).push({
        google_ad_client: '{{google_adsense_id}}',
        enable_page_level_ads: true
    });
    {{/adsense}}
</script>
